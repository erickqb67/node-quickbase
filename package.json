{
  "name": "quickbase",
  "version": "3.0.0-alpha.8",
  "description": "A lightweight, very flexible QuickBase API",
  "keywords": [
    "QuickBase",
    "quickbase",
    "Quick Base",
    "quick base",
    "qb",
    "api"
  ],
  "homepage": "https://github.com/tflanagan/node-quickbase",
  "bugs": {
    "url": "https://github.com/tflanagan/node-quickbase/issues",
    "email": "contact@tristianflanagan.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Tristian Flanagan",
    "url": "https://github.com/tflanagan",
    "email": "contact@tristianflanagan.com"
  },
  "maintainers": [
    {
      "name": "tflanagan",
      "url": "https://github.com/tflanagan",
      "email": "contact@tristianflanagan.com"
    }
  ],
  "contributors": [
    "Tristian Flanagan <contact@tristianflanagan.com> (https://github.com/tflanagan)"
  ],
  "main": "./dist/quickbase.js",
  "types": "./dist/quickbase.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/tflanagan/node-quickbase.git"
  },
  "scripts": {
    "cleanup": "rm -rf ./dist/*",
    "reinstall": "rm -rf node_modules/ && npm install",
    "reinstall-dev": "rm -rf node_modules/ && npm install && npm install --dev",
    "build": "npx tsc && browserify ./dist/quickbase.js > ./dist/quickbase.browserify.js && minify ./dist/quickbase.browserify.js > ./dist/quickbase.browserify.min.js && rm ./dist/quickbase.browserify.js",
    "docs": "mv ./docs/_config.yml ./ && npx typedoc src/quickbase.ts && mv ./_config.yml ./docs/",
    "test": "npx ava",
    "prepublishOnly": "npm run cleanup && npm run build && npm run test"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "axios": "^0.19.2",
    "debug": "^4.1.1",
    "generic-throttle": "^2.0.1",
    "merge": "github:yeikos/js.merge"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^13.11.1",
    "ava": "^3.7.0",
    "browserify": "^16.5.1",
    "dotenv": "^8.2.0",
    "minify": "^5.1.1",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "git": "git://github.com/tflanagan/node-quickbase.git"
}
